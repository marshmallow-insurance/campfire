{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": "libs",
    "baseUrl": "libs",
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "./dist"
  },
  "exports": {
    "./configs": {
      "import": "./dist/configs/*",
      "types": "./dist/configs/*"
    }
  },
  "exclude": [
    "release.config.js",
    "commitlint.config.ts",
    "eslint.config.js",
    "prettier.config.js",
    "lint-staged.config.js"
  ]
}
